{
  "name": "ewsjs-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ewsjs/xhr": "^1.4.0",
    "electron-util": "^0.12.1",
    "ews-javascript-api": "^0.9.6",
    "react-data-grid": "^6.1.0",
    "react-split-pane": "^0.1.87"
  },
  "main": "./build/electron.js",
  "scripts": {
    "start": "rescripts start",
    "build": "cross-env PUBLIC_URL=./ rescripts build && tsc -p tsconfig.build.json",
    "test": "rescripts test",
    "electron": "electron .",
    "electron:dev": "electron ./electron.dev.js",
    "electron-start": "node src/electron-wait-react",
    "react-start": "rescripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-env": "^0.0.10",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.1",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "cross-env": "^5.2.1",
    "dotenv": "^8.1.0",
    "electron": "^6.0.1",
    "electron-builder": "^21.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.3",
    "office-ui-fabric-react": "^7.28.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.0.1",
    "ts-node": "^8.3.0",
    "typescript": "3.5.3"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "extends": null,
    "files": [
      "./build/**/*",
      "./build/electron.js"
    ],
    "win": {
      "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
